{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env EXPO_NO_TELEMETRY=1 npx expo start",
    "start": "expo start",
    "build:web": "cross-env EXPO_NO_TELEMETRY=1 npx expo export -p web --output-dir build",
    "prepare:publish": "shx rm -rf public dist && shx mkdir -p public dist && shx cp -r build/* public/ && shx cp -r build/* dist/ && echo '/* /index.html 200' > public/_redirects && echo '<!doctype html><meta http-equiv=\"refresh\" content=\"0; url=/public/\">' > index.html",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.55.0",
    "expo": "53.0.20",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.540.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "react-native-worklets": "^0.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@types/react": "~19.0.10",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.0.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "metro": "^0.82.0",
    "shx": "^0.4.0",
    "typescript": "~5.8.3"
  }
}
